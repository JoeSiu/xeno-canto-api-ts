{
  "name": "xeno-canto-api-ts",
  "version": "1.0.3",
  "description": "A TypeScript wrapper for the Xeno Canto API",
  "homepage": "https://joesiu.github.io/xeno-canto-api-ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoeSiu/xeno-canto-api-ts"
  },
  "bugs": {
    "url": "https://github.com/JoeSiu/xeno-canto-api-ts/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "xeno-canto",
    "API",
    "bird",
    "grasshopper",
    "bat",
    "wildlife",
